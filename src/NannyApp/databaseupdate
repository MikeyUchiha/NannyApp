IF NOT EXISTS(SELECT * FROM [__EFMigrationsHistory] WHERE [MigrationId] = N'20160316043654_FilePaths4')
BEGIN
    DECLARE @var3 sysname;
    SELECT @var3 = [d].[name]
    FROM [sys].[default_constraints] [d]
    INNER JOIN [sys].[columns] [c] ON [d].[parent_column_id] = [c].[column_id]
    WHERE ([d].[parent_object_id] = OBJECT_ID(N'FilePath') AND [c].[name] = N'FamilyId');
    IF @var3 IS NOT NULL EXEC(N'ALTER TABLE [FilePath] DROP CONSTRAINT [' + @var3 + ']');
    ALTER TABLE [FilePath] ALTER COLUMN [FamilyId] int;
END

GO

