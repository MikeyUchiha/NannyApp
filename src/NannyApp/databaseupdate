IF NOT EXISTS(SELECT * FROM [__EFMigrationsHistory] WHERE [MigrationId] = N'20160315040937_FilePaths')
BEGIN
    CREATE TABLE [FilePath] (
        [Id] int NOT NULL IDENTITY,
        [ActivityId] int NOT NULL,
        [ChildId] int NOT NULL,
        [DateUploaded] datetime2 NOT NULL,
        [FamilyId] int NOT NULL,
        [FileName] nvarchar(max),
        [FileType] int NOT NULL,
        [UserId] nvarchar(450),
        CONSTRAINT [PK_FilePath] PRIMARY KEY ([Id]),
        CONSTRAINT [FK_FilePath_Activity_ActivityId] FOREIGN KEY ([ActivityId]) REFERENCES [Activity] ([Id]) ON DELETE CASCADE,
        CONSTRAINT [FK_FilePath_Child_ChildId] FOREIGN KEY ([ChildId]) REFERENCES [Child] ([Id]) ON DELETE CASCADE,
        CONSTRAINT [FK_FilePath_Family_FamilyId] FOREIGN KEY ([FamilyId]) REFERENCES [Family] ([Id]) ON DELETE CASCADE,
        CONSTRAINT [FK_FilePath_ApplicationUser_UserId] FOREIGN KEY ([UserId]) REFERENCES [AspNetUsers] ([Id]) ON DELETE NO ACTION
    );
END

